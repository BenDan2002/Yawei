<?xml version='1.0' encoding='utf-8'?>
<plugin id="yawei.sportal" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Sportal</name>
    <js-module name="Sportal" src="www/Sportal.js">
        <clobbers target="cordova.plugins.Sportal" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="Sportal"><param name="android-package" value="yawei.sportal.Sportal" /></feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml">
        </config-file>
        <source-file src="src/android/Sportal.java" target-dir="src/yawei/sportal/Sportal" />
    </platform>
</plugin>